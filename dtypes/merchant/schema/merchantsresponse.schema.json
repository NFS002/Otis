{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://gitlab.com/otis_team/backend/dtypes/merchant/schema/merchantsresponse.schema.json",
  "title": "Merchants Response",
  "description": "A list of partner merchants and a list of general merchants returned from a merchant query",
  "$async": true,
  "type": "object",
  "properties": {
    "partnerMerchants": {
      "type": "array",
      "items": {
        "$ref": "http://gitlab.com/otis_team/backend/dtypes/partnermerchant/schema/partnermerchant.schema.json"
      },
      "additionalItems": false
    },
    "generalMerchants": {
      "type": "array",
      "items": {
        "$ref": "http://gitlab.com/otis_team/backend/dtypes/generalmerchant/schema/generalmerchant.schema.json"
      },
      "additionalItems": false
    },
    "executed": {
      "description": "A boolean flag to indicate whether or not the query was successfully executed",
      "type": "boolean"
    }
  },
  "required": ["executed", "generalMerchants", "partnerMerchants"],
  "additionalProperties": false
}